{
  "name": "seed-template-nestjs-typeorm",
  "version": "0.0.1.000002-beta",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "erdia": "cross-env NODE_ENV=development erdia",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "seed": "ts-node src/database/seeds/run-seed.ts",
    "seed:dev": "cross-env NODE_ENV=development npm run seed",
    "seed:prod": "cross-env NODE_ENV=production npm run seed",
    "typeorm": "node scripts/typeorm-run.js",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:show": "npm run typeorm migration:show",
    "migration:create": "npm run typeorm migration:create ./src/migrations/Migration",
    "migration:generate": "npm run typeorm migration:generate ./src/migrations/Migration",
    "generate:alias": "ts-node lib/generate.ts",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:dev:local": "cross-env NODE_ENV=development RUN_ON=local nest start --watch",
    "start:test:local": "cross-env NODE_ENV=test RUN_ON=local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production RUN_ON=cloud node dist/main",
    "start:prod:local": "cross-env NODE_ENV=production RUN_ON=local nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test RUN_ON=local vitest -c ./vitest.config.mjs",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "init:db": "winpty dotenv -e .env.test.local -- sh scripts/test_db_initialize",
    "previtest:e2e": "npm run init:db",
    "jest:e2e": "cross-env NODE_ENV=test RUN_ON=local jest -c ./test/jest-e2e.json --detectOpenHandles",
    "vitest:e2e": "cross-env NODE_ENV=test vitest run -c ./test/vitest.config.mjs",
    "vitest:e2e:watch": "cross-env NODE_ENV=test vitest run -c ./test/vitest.config.mjs",
    "version:deploy:up": "sh scripts/deploy_version_up",
    "version:deploy:init": "sh scripts/deploy_version_init"
  },
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
    "@faker-js/faker": "^10.0.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cross-env": "^10.0.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.18",
    "ejs": "^3.1.10",
    "image-size": "^2.0.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.5",
    "nanoid": "^5.1.5",
    "nest-winston": "^1.10.2",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.3",
    "typeorm": "^0.3.26",
    "typeorm-extension": "^3.7.1",
    "typeorm-naming-strategies": "^4.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.30",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@swc/jest": "^0.2.39",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.4.0",
    "@types/nodemailer": "^7.0.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/sharp": "^0.32.0",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "dotenv": "^17.2.2",
    "erdia": "^3.7.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "unplugin-swc": "^1.5.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s?$": [
        "@swc/jest"
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
